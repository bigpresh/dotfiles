function mute {
    player=$1
    period=$2
    if [[ "$period" == "" ]]; then
        period="10m"
    fi
    sudo msm The-Wild cmd mute $player $period
}

function tempban {
    player=$1
    period=$2
    if [[ "$period" == "" ]]; then
        period="1d"
    fi
    sudo msm The-Wild cmd tempban $player $period
}

function ban {
    player=$1
    reason=$2
    sudo msm The-Wild cmd tempban $player $reason
}

